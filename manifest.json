{
    "manifest_version": 2,
    "name": "Auto Apply Clicker",
    "version": "1.1",
    "description": "Automatically clicks the 'Apply' button every 2 seconds.",
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  